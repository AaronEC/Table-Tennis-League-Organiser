<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480" prefWidth="720" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.AdminController">
      <children>
      <TabPane prefHeight="480.0" prefWidth="720.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Teams">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ListView fx:id="teamView" layoutX="26.0" layoutY="63.0" prefHeight="340.0" prefWidth="222.0" />
                        <ChoiceBox fx:id="leagueChoiceBox" layoutX="26.0" layoutY="22.0" prefHeight="26.0" prefWidth="222.0" />
                        <TableView fx:id="leagueTable" layoutX="280.0" layoutY="63.0" prefHeight="323.0" prefWidth="191.0">
                           <columns>
                              <TableColumn fx:id="leagueRank" prefWidth="35.20001220703125" text="Rank" />
                              <TableColumn fx:id="leagueName" prefWidth="100.79998779296875" text="Name" />
                              <TableColumn fx:id="leaguePoints" prefWidth="54.40001220703124" text="Points" />
                           </columns>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </TableView>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Fixtures">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab text="Scores">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ListView layoutX="38.0" layoutY="62.0" prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
      <Button fx:id="logout" layoutX="651.0" layoutY="440.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
   </children>
</AnchorPane>
