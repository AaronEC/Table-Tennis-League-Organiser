<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480" prefWidth="720" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.AdminController">
      <children>
      <TabPane prefHeight="480.0" prefWidth="720.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab text="Leagues">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="leagueTableAdmin" layoutX="36.0" layoutY="35.0" prefHeight="314.0" prefWidth="272.0">
                           <columns>
                              <TableColumn fx:id="leagueNameAdmin" prefWidth="208.0" text="Name" />
                              <TableColumn fx:id="leagueTeamsAdmin" minWidth="0.0" prefWidth="61.60003662109375" text="Teams" />
                           </columns>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </TableView>
                        <Button layoutX="360.0" layoutY="156.0" mnemonicParsing="false" onAction="#editLeagueName" prefHeight="26.0" prefWidth="150.0" text="Change Name" />
                        <Button fx:id="deleteLeague" layoutX="364.0" layoutY="211.0" mnemonicParsing="false" onAction="#deleteLeague" prefHeight="26.0" prefWidth="147.0" text="Delete Selected League" />
                        <TextField fx:id="leagueEditName" layoutX="360.0" layoutY="130.0" />
                        <Button layoutX="359.0" layoutY="84.0" mnemonicParsing="false" onAction="#addLeague" prefHeight="26.0" prefWidth="150.0" text="Add League" />
                        <TextField fx:id="leagueNameIn" layoutX="359.0" layoutY="58.0" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
          <Tab text="Teams">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ChoiceBox fx:id="leagueChoiceBox" layoutX="26.0" layoutY="22.0" prefHeight="26.0" prefWidth="189.0" />
                        <TableView fx:id="teamTable" layoutX="26.0" layoutY="63.0" prefHeight="314.0" prefWidth="191.0">
                           <columns>
                              <TableColumn fx:id="teamRank" prefWidth="35.20001220703125" text="Rank" />
                              <TableColumn fx:id="teamName" prefWidth="100.79998779296875" text="Name" />
                              <TableColumn fx:id="teamPoints" prefWidth="54.40001220703124" text="Points" />
                           </columns>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </TableView>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Fixtures">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab text="Scores">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ListView layoutX="38.0" layoutY="62.0" prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
      <Button fx:id="logout" layoutX="651.0" layoutY="440.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
   </children>
</AnchorPane>
